<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <!-- default inside container -->
    <add key="globalPackagesFolder" value="/root/.nuget/packages" />
  </config>
  <fallbackPackageFolders>
    <!-- remove VS fallback like C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages -->
    <clear />
  </fallbackPackageFolders>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
</configuration>
